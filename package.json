{
  "name": "sodium-native-no-prebuild",
  "version": "2.2.1-1",
  "description": "Low level bindings for libsodium",
  "main": "index.js",
  "dependencies": {
    "ini": "^1.3.5",
    "nan": "^2.4.0",
    "prebuild-install": "^5.0.0",
    "bindings": "~1.3.0"
  },
  "devDependencies": {
    "node-gyp": "^3.6.1",
    "prebuild": "8.0.1",
    "prebuild-ci": "^2.0.0",
    "sodium-test": "^0.7.0",
    "sodium-vectors": "^1.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  },
  "scripts": {
    "dev": "node-gyp rebuild",
    "fetch-libsodium": "git submodule update --recursive --init",
    "test": "standard && tape \"test/*.js\"",
    "prebuild": "prebuild --all --verbose --strip --preinstall \"node preinstall.js\" && node postinstall.js",
    "install": "node preinstall.js && prebuild-install || node-gyp rebuild && node postinstall.js",
    "rebuild": "prebuild --compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/staltz/sodium-native-no-prebuild.git"
  },
  "contributors": [
    "Emil Bay <github@tixz.dk> (http://bayes.dk)",
    "Mathias Buus (@mafintosh)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/staltz/sodium-native-no-prebuild/issues"
  },
  "homepage": "https://github.com/staltz/sodium-native-no-prebuild"
}